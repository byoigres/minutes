{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
    }
  },
  "globals": {
    "h": true
  },
  "env": {
    "browser": true
  },
  "rules": {
    "no-unused-vars": [2, { "varsIgnorePattern": "^h$" }],
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/react-in-jsx-scope": 0,
    "global-require": 0,
    "import/no-unresolved": [
      2, {
        "ignore": [
          "^react$",
          "^react-dom$",
          "^./styles$",
          "^components/"
        ]
      }
    ]
  }
}
